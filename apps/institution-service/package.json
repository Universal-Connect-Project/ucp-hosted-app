{
  "name": "institution-service",
  "version": "1.0.0",
  "description": "Insitution service",
  "scripts": {
    "test": "jest --runInBand",
    "test:watch": "jest --runInBand --watch",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "db:generateMigration": "npx sequelize-cli migration:generate",
    "db:setup": "npm run db:create && npm run db:migrate && npm run db:load",
    "db:setup:minimal": "npm run db:create && npm run db:migrate && npm run db:load:minimal",
    "db:create": "npx sequelize-cli db:create",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:reset": "npx sequelize-cli db:drop && npm run db:setup",
    "db:reset:minimal": "npx sequelize-cli db:drop && npm run db:setup:minimal",
    "db:load": "npx ts-node ./src/tasks/loadInstitutionsFromJson.ts",
    "db:load:minimal": "npx ts-node ./src/tasks/loadAggregators.ts",
    "test:db:setup": "npm run test:db:create && npm run test:db:migrate && npm run test:db:seed",
    "test:db:create": "npx sequelize-cli db:create --env test",
    "test:db:migrate": "npx sequelize-cli db:migrate --env test",
    "test:db:seed": "npx sequelize-cli db:seed:all --env test",
    "test:db:reset": "npx sequelize-cli db:drop --env test && npm run test:db:setup",
    "dev": "nodemon ./src/app.ts",
    "dev:noRateLimiting": "DISABLE_RATE_LIMITING=true npm run dev",
    "dev:noRateLimiting:limitSyncRequests": "DISABLE_RATE_LIMITING=true E2E_LIMIT_SYNC_REQUESTS=true npm run dev",
    "prod": "PRODUCTION=true npm run dev",
    "lint": "eslint . --ext .ts --report-unused-disable-directives --max-warnings 0",
    "prettier": "prettier . --check"
  },
  "dependencies": {
    "@repo/backend-utils": "*",
    "@repo/shared-utils": "*",
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "cron": "^4.3.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-oauth2-jwt-bearer": "^1.6.0",
    "express-rate-limit": "^7.4.0",
    "fastest-levenshtein": "^1.0.16",
    "https-proxy-agent": "^7.0.6",
    "joi": "^17.13.3",
    "jwt-decode": "^4.0.0",
    "pg": "^8.12.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.3",
    "sequelize-cli": "^6.6.2",
    "sequelize-typescript": "^2.1.6",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.35.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.14.12",
    "@types/uuid": "^10.0.0",
    "@types/validator": "^13.12.0",
    "cypress": "^15.1.0",
    "eslint": "^9.35.0",
    "eslint-plugin-cypress": "^3.0.3",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^30.2.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "msw": "^2.11.1",
    "nodemon": "^3.1.10",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
