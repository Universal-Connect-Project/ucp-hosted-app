    "/events/{connectionId}/connectionStart": {
      "post": {
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "description": "Begin tracking the bank connection process",
        "parameters": [
          {
            "in": "path",
            "name": "connectionId",
            "schema": {
              "type": "string"
            },
            "required": true,
            "description": "Connection ID from assigned aggregator"
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "jobTypes": {
                    "type": "array",
                    "items": {
                      "type": "string",
                      "enum": [
                        "accountNumber",
                        "accountOwner",
                        "transactions",
                        "transactionHistory"
                      ]
                    }
                  },
                  "institutionId": {
                    "type": "string"
                  },
                  "aggregatorId": {
                    "type": "string"
                  },
                  "recordDuration": {
                    "type": "boolean",
                    "default": true
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Created",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "example": {
                    "message": "Connection Tracking started",
                    "event": {
                      "connectionId": "MBR-123-455-123",
                      "institutionId": "13213-12312-123",
                      "aggregatorId": "mx",
                      "recordDuration": true,
                      "clientId": "54321",
                      "jobTypes": ["transactions"],
                      "startedAt": "2024-09-10T16:07:46.038Z"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "example": {
                    "error": "\"institutionId\" is required"
                  }
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized"
          }
        }
      }
    },
