        ],
        "description": "All performance data for the last 30 days",
        "parameters": [],
        "responses": {
          "200": {
            "description": "Created",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "example": {
                    "cbc051c6-55c0-40fd-8e90-d88ec9866164": {
                      "transactions": {
                        "successRate": {
                          "mx": 71,
                          "sophtron": 70,
                          "finicity": 100
                        },
                        "jobDuration": {
                          "mx": 40.06266666666666666,
                          "sophtron": 114.067,
                          "testAggregatorId2": 56.374
                        }
                      },
                      "accountNumber": {
                        "successRate": {
                          "mx": 50
                        },
                        "jobDuration": {}
                      },
                      "accountNumber,transactions": {
                        "successRate": {
                          "sophtron": 55
                        },
                        "jobDuration": {}
                      }
                    },
                    "ae94909a-c798-4ef2-8bd2-65b2d3cafa43": {
                      "transactions": {
                        "successRate": {
                          "mx": 66.67
                        },
                        "jobDuration": {
                          "mx": 30.5325
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized"
          }
        }
      }
    },
    "/metrics/aggregatorSuccessGraph": {
      "get": {
        "summary": "Fetch aggregator success graph metrics",
        "description": "Retrieves success graph metrics based on provided time frame, job types, and aggregators.",
        "parameters": [
          {
            "name": "timeFrame",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string",
              "enum": ["1d", "1w", "30d", "180d", "1y"]
            },
            "description": "Time frame for the metrics (e.g., '30d'). Defaults to 30d."
          },
          {
            "name": "jobTypes",
            "in": "query",
            "required": false,
            "schema": {
              "type": "array",
              "items": {
                "type": "string"
              },
              "example": [
                "transactions",
                "transactions|accountOwner",
                "accountNumber|transactions"
              ]
            },
            "description": "A comma-separated list of job types. Some job types can be separated by '|'. Defaults to no filtering by jobTypes."
          },
          {
            "name": "aggregators",
            "in": "query",
            "required": false,
            "schema": {
              "type": "array",
              "items": {
                "type": "string"
              },
              "example": ["sophtron", "mx"]
            },
            "description": "A comma-separated list of aggregators. Defaults to all aggregators."
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "example": {
                  "sophtron": [
                    {
                      "date": "2024-03-01T00:00:00Z",
                      "value": 1
                    },
                    {
                      "date": "2024-03-02T00:00:00Z",
                      "value": 0.95
                    }
                  ],
                  "mx": [
                    {
                      "date": "2024-03-01T00:00:00Z",
                      "value": 0.85
                    },
                    {
                      "date": "2024-03-02T00:00:00Z",
                      "value": 0.87
                    }
                  ]
                }
              }
            }
          },
          "400": {
            "description": "Invalid query parameters",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": {
                      "type": "string"
                    }
                  }
                },
                "example": {
                  "error": "\"timeFrame\" must be one of [1d, 1w, 30d, 180d, 1y]"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized"
          }
        }
      }
    },
    "/metrics/aggregatorDurationGraph": {
      "get": {
        "summary": "Fetch aggregator duration graph metrics",
        "description": "Retrieves duration graph metrics based on provided time frame, job types, and aggregators.",
        "parameters": [
          {
            "name": "timeFrame",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string",
              "enum": ["1d", "1w", "30d", "180d", "1y"]
            },
            "description": "Time frame for the metrics (e.g., '30d'). Defaults to 30d."
          },
          {
            "name": "jobTypes",
            "in": "query",
            "required": false,
            "schema": {
              "type": "array",
              "items": {
                "type": "string"
              },
              "example": [
                "transactions",
                "transactions|accountOwner",
                "accountNumber|transactions"
              ]
            },
            "description": "A comma-separated list of valid job types. Some job types can be separated by '|'. Defaults to no filtering by jobTypes."
          },
          {
            "name": "aggregators",
            "in": "query",
            "required": false,
            "schema": {
              "type": "array",
              "items": {
                "type": "string"
              },
              "example": ["sophtron", "mx"]
            },
            "description": "A comma-separated list of aggregators. Defaults to all aggregators."
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "additionalProperties": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "properties": {
                        "date": {
                          "type": "string",
                          "format": "date-time",
                          "example": "2024-03-01T00:00:00Z"
                        },
                        "value": {
                          "type": "integer",
                          "description": "Time in milliseconds.",
                          "example": 1000
                        }
                      }
                    }
                  }
                },
                "example": {
                  "sophtron": [
                    {
                      "date": "2024-03-01T00:00:00Z",
                      "value": 1500
                    },
                    {
                      "date": "2024-03-02T00:00:00Z",
                      "value": 1000
                    }
                  ],
                  "mx": [
                    {
                      "date": "2024-03-01T00:00:00Z",
                      "value": 1200
                    },
                    {
                      "date": "2024-03-02T00:00:00Z",
                      "value": 2500
                    }
                  ]
                }
              }
            }
          },
          "400": {
            "description": "Invalid query parameters",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": {
                      "type": "string"
                    }
                  }
                },
                "example": {
                  "error": "\"timeFrame\" must be one of [1d, 1w, 30d, 180d, 1y]"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized"
          }
        }
      }
    },
    "/events/{connectionId}/connectionStart": {
      "post": {
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "description": "Begin tracking the bank connection process",
        "parameters": [
          {
            "in": "path",
            "name": "connectionId",
            "schema": {
              "type": "string"
            },
            "required": true,
            "description": "Connection ID from assigned aggregator"
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "jobTypes": {
                    "type": "array",
                    "items": {
                      "type": "string",
                      "enum": [
                        "accountNumber",
                        "accountOwner",
                        "transactions",
                        "transactionHistory"
                      ]
                    }
                  },
                  "institutionId": {
                    "type": "string"
                  },
                  "aggregatorId": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Created",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "example": {
                    "message": "Connection Tracking started",
                    "event": {
                      "connectionId": "MBR-123-455-123",
                      "institutionId": "13213-12312-123",
                      "aggregatorId": "mx",
                      "clientId": "54321",
                      "jobTypes": ["transactions"],
                      "startedAt": "2024-09-10T16:07:46.038Z"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "example": {
                    "error": "\"institutionId\" is required"
                  }
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized"
          }
        }
      }
    },
    "/events/{connectionId}/connectionPause": {
      "put": {
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "description": "Pause the duration tracking on a connection",
        "parameters": [
          {
            "in": "path",
            "name": "connectionId",
            "schema": {
              "type": "string"
            },
            "required": true,
            "description": "Connection ID from assigned aggregator"
          }
        ],
        "responses": {
          "200": {
            "description": "Updated",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "example": {
                    "message": "Duration tracking paused",
                    "event": {
                      "connectionId": "MBR-123-455-123",
                      "institutionId": "13213-12312-123",
                      "aggregatorId": "mx",
                      "clientId": "54321",
                      "jobTypes": ["transactions"],
                      "userInteractionTime": 0,
                      "pausedAt": "2024-09-10T16:07:51.038Z",
                      "startedAt": "2024-09-10T16:07:46.038Z",
                      "successAt": null
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "example": {
                    "error": "\"id\" was not found"
                  }
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized"
          }
        }
      }
    },
    "/events/{connectionId}/connectionResume": {
      "put": {
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "description": "Resume the duration tracking on a connection",
        "parameters": [
          {
            "in": "path",
            "name": "connectionId",
            "schema": {
              "type": "string"
            },
            "required": true,
            "description": "Connection ID from assigned aggregator"
          }
        ],
        "responses": {
          "200": {
            "description": "Updated",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "example": {
                    "message": "Duration tracking resumed",
                    "event": {
                      "connectionId": "MBR-123-455-123",
                      "institutionId": "13213-12312-123",
                      "aggregatorId": "mx",
                      "clientId": "54321",
                      "jobTypes": ["transactions"],
                      "userInteractionTime": 140,
                      "pausedAt": "2024-09-10T16:07:51.038Z",
                      "startedAt": "2024-09-10T16:07:46.038Z",
                      "successAt": null
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "example": {
                    "error": "\"id\" was not found"
                  }
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized"
          }
        }
      }
    },
    "/events/{connectionId}/connectionSuccess": {
      "put": {
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "description": "Record the moment a connection was successful",
        "parameters": [
          {
            "in": "path",
            "name": "connectionId",
            "schema": {
              "type": "string"
            },
            "required": true,
            "description": "Connection ID from assigned aggregator"
          }
        ],
        "responses": {
          "200": {
            "description": "Updated",
