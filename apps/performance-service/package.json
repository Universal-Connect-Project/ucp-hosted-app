{
  "name": "performance-service",
  "version": "1.0.0",
  "description": "Performance service",
  "scripts": {
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "dev": "concurrently --kill-others-on-fail \"nodemon ./src/app.ts\" \"npm run redisserver\" \"npm run start:influxDb\"",
    "start": "nodemon ./src/app.ts",
    "prod": "NODE_ENV=production npm run dev",
    "lint": "eslint . --ext .ts --report-unused-disable-directives --max-warnings 0",
    "redisserver": "sh ./scripts/start-redis.sh",
    "prettier": "prettier . --check",
    "test": "jest --runInBand",
    "start:influxDb": "sh ./scripts/start-influx.sh",
    "test:seedInfluxPerformance": "ts-node ./src/shared/tests/scripts/seedInfluxPerformance.ts",
    "test:watch": "jest --watch --runInBand"
  },
  "dependencies": {
    "@influxdata/influxdb-client": "^1.35.0",
    "@repo/backend-utils": "*",
    "@repo/shared-utils": "*",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.19.2",
    "express-oauth2-jwt-bearer": "^1.6.0",
    "express-rate-limit": "^7.4.0",
    "joi": "^17.13.3",
    "jwt-decode": "^4.0.0",
    "lodash.groupby": "^4.6.0",
    "lodash.intersection": "^4.4.0",
    "redis": "^4.7.0",
    "set-interval-async": "^3.0.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.35.0",
    "@influxdata/influxdb-client-apis": "^1.35.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/lodash.groupby": "^4.6.9",
    "@types/node": "^20.14.12",
    "concurrently": "^9.1.2",
    "cypress": "^15.1.0",
    "eslint": "^9.35.0",
    "eslint-plugin-cypress": "^3.0.3",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-prettier": "^5.1.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "msw": "^2.11.1",
    "nodemon": "^3.1.10",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
