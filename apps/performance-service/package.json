{
  "name": "performance-service",
  "version": "1.0.0",
  "description": "Performance service",
  "scripts": {
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "dev": "concurrently --kill-others \"nodemon ./src/app.ts\" \"npm run redisserver\"",
    "start:e2e": "nodemon ./src/app.ts",
    "prod": "PRODUCTION=true npm run dev",
    "lint": "eslint . --ext ts --report-unused-disable-directives --max-warnings 0",
    "redisserver": "docker run --rm --name ucw_redis -p 6379:6379 redis:7.2-alpine",
    "prettier": "prettier . --check",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@repo/backend-utils": "*",
    "@repo/shared-utils": "*",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.19.2",
    "express-oauth2-jwt-bearer": "^1.6.0",
    "express-rate-limit": "^7.4.0",
    "joi": "^17.13.3",
    "redis": "^4.7.0",
    "set-interval-async": "^3.0.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.12",
    "concurrently": "^9.1.2",
    "cypress": "^13.13.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
